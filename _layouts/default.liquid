<!doctype html>
<html lang="de">
<head>
    <title>{{ page.title }}</title>
    <meta charset="utf-8">
    <meta name="description" content="Superrr - No Agenda. No Men*. No Bullshit.">
    <meta name="author" content="Rainbow Unicorn">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="{{ "/assets/main.css?v=2.5" | relative_url }}">
    {% include favicon-superrr.liquid %}
    {% include opengraph-superrr.liquid %}
</head>
<body>
<div class="loader">
    <span>SUPERRR.NET</span>
</div>
<div id="barba-wrapper">
    <div class="barba-container">
        {% assign page_type = '' %}
        {% if page.type != ''%}
            {% assign page_type = page.type %}
        {% endif %}
        {% if page.id != 'home' %}
            {% include header.liquid page_type=page_type %}
        {% endif %}
        <div class="page-wrap col-xs-24 {{ page_type }}">
            {{ content }}
        </div>
    </div>
</div>

{% include footer.liquid%}



