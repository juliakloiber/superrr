<div class="navigation-left-wrap">
    <div class="navigation-left-open-wrap">
        <div class="nav-open-btn lab">
            {% include partials/nav-icon-open.liquid  %}
        </div>
        <!--<div class="nav-open-info">
            <span class="open">MENUE</span>
        </div>-->
    </div>
    <div class="navigation-left-wrap-inner">
        <div class="nav-close-btn lab">
            {% include partials/nav-icon-close.liquid  %}
        </div>

        <ul class="main-nav {{ include.page_type }}">
            {% assign base_dir = page.url | split:'/'  %}
            {% assign base_dir = base_dir  %}
            <li {% if base_dir[1] == 'about' %} class="current" {% endif %}><a class="fontsize-50 rounded-hover" href="/about">ABOUT</a></li>
            <li {% if base_dir[1] == 'projects' %} class="current" {% endif %}><a class="fontsize-50 rounded-hover" href="/projects">LAB</a></li>
            <!-- <li {% if base_dir[1] == 'feministtech' %} class="current" {% endif %}><a class="fontsize-50 rounded-hover" href="/feministtech">FEMINIST TECH</a></li> -->
            <li {% if base_dir[1] == 'community' %} class="current" {% endif %}><a class="fontsize-50 rounded-hover" href="/community">COMMUNITY</a></li>
            <li {% if base_dir[1] == 'news' %} class="current" {% endif %}><a class="fontsize-50 rounded-hover" href="/news">NEWS</a></li>
        </ul>
        <div class="community-batch-wrap">
            <a href="{{ site.support_us_link }}" class="batch-inner support">
                                        <div class="img-responsive batch-svg" >
                                            {% include svgs/superrr-support.liquid %}
                                        </div>
                                    </a>
        </div>
    </div>
</div>
